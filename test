print("*****Test basic arithmetic*****");
print("  4 + 1:    " + (4 + 1));
print("  Expected: 5");
print("  3 * 5:    " + (3 * 5));
print("  Expected: 15");
print("  10 / 4:   " + (10 / 4));
print("  Expected: 2.5");
print("  10 % 4:   " + (10 % 4));
print("  Expected: 2");
print("*****Test complex arithmetic*****");
print("  3*(4 + 1):            " + (3*(4 + 1)));
print("  Expected:             15");
print("  +3 * 2 + 10 / 2:      " + (+3 * 2 + 10 / 2));
print("  Expected:             11");
print("  -(1 - 5)*(4 + 1) % 7: " + (-(1 - 5)*(4 + 1) % 7));
print("  Expected:             6");
print("*****Test string handling*****");
print("  \"abc\" + \"def\": " + ("abc" + "def"));
print("  Expected:      abcdef");
print("  \"abc\" + 123:   " + ("abc" + 123));
print("  Expected:      abc123");
print("  \"\\\"abc\\\"\":     " + ("\"abc\""));
print("  Expected:      \"abc\"");
print("  \"\\\\abc\\\\\":     " + ("\\abc\\"));
print("  Expected:      \\abc\\");
print("*****Test boolean handling*****");
print("  true * true:  " + (true * true));
print("  Expected:     1");
print("  true * false: " + (true * false));
print("  Expected:     0");
print("*****Array handling*****");
print("  ********Setup********  ");
a=[];
a[0] = 1;
a[1 + 1] = "hi";
b = [true, 4 + 5, , "test"];
b[1] = 2 * 2 * 2;
print("       a=[]");
print("       a[0] = 1");
print("       a[1 + 1] = \"hi\"");
print("       b = [true, 4 + 5, , \"test\"]");
print("       b[1] = 2 * 2 * 2");
print("  ********Test********  ");
print("  a:        " + string(a));
print("  Expected: [1,null,\"hi\"]");
print("  b:        " + string(b));
print("  Expected: [true,8,null,\"test\"]");
print("  b[1]:     " + string(b[1]));
print("  Expected: 8");
print("*****Hash handling*****");
print("  *******Setup*******  ");
c={};
c.key = 1;
c["test 123"] = "hi";
d = {a: true, b: 4 + 5, a_b123: "test"};
d.b = 2 * 2 * 2;
d["hmm" + " mm"] = 2 * 2 * 2;
print("       c={}");
print("       c.key = 1");
print("       c[\"test 123\"] = \"hi\"");
print("       d = {a: true, b: 4 + 5, a_b123: \"test\"}");
print("       d.b = 2 * 2 * 2");
print("       d[\"hmm\" + \" mm\"] = 2 * 2 * 2");
print("  *******Test*******  ");
print("  c:           " + string(c));
print("  Expected:    {\"key\":1,\"test 123\":\"hi\"}");
print("  d:           " + string(d));
print("  Expected:    {\"a\":true,\"b\":9,\"a_b123\":\"test\",\"hmm mm\":8}");
print("  d[\"hmm mm\"]: " + string(d["hmm mm"]));
print("  Expected:    8");
